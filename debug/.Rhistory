d <- read.csv('results.csv',header=T)
powered <- subset(d,d$powerDenom>5&d$baseDenom>5&d$percentDiff<0.99)
trueFalse <- subset(powered,verifiedSpeaker=="True"&verifiedReplier=="False"&percentDiff>0.8389)
falseTrue <- subset(powered,verifiedSpeaker=="False"&verifiedReplier=="True"&percentDiff<0.3)
hist(trueFalse$alignment, col=rgb(1,0,0,0.5),freq=FALSE,main="")
hist(falseTrue$alignment, col=rgb(0,0,1,0.5),freq=FALSE,add=T)
legend("topright", c("trueFalse+largeSmall", "falseTrue+smallLarge"), col=c("red", "blue"), lwd=10)
